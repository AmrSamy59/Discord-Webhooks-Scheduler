{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {"includeFiles": ["index.js", "package.json", "package-lock.json"]}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "cron": {
        "schedule": "* * * * *",
        "command": "cron.js"
    }
}
